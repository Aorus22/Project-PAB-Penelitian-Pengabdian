<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DashbordPenelitianDanPengabdian" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">#234373</item>
        <item name="colorOnPrimary">@color/white</item>
    </style>

    <style name="Theme.DashbordPenelitianDanPengabdian.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DashbordPenelitianDanPengabdian.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DashbordPenelitianDanPengabdian.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="MenuDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/dividerHorizontal</item> <!-- Use the system divider color -->
    </style>
</resources>